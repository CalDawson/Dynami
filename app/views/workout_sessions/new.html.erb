<div>
  <h1>Workout_session new page</h1>
</div>


<div class="container">
  <%= simple_form_for(@session) do |f| %>
    <%= f.association :workout, prompt: 'Select a list'%>
    <%= f.input :time %>
    <%= f.submit value: 'Add to list', class:'btn btn-primary' %>
  <% end %>
</div>

<div class="container">
  <%= simple_form_for(@workout) do |f| %>
    <%= f.input :name %>
    <%= f.input :workout_type %>
    <%= f.submit value: 'Create New Workout', class:'btn btn-primary' %>
  <% end %>
</div>
